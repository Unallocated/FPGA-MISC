A=0; for i in {1..255}; do A=$(echo $i | perl -e '$_ = <>; printf("%08b", int($_));'); cat fft_test.vhd | sed "s/dds_data <= \"........\"/dds_data <= \"$A\"/" > fft_test.vhd.new; mv fft_test.vhd.new fft_test.vhd; /opt/Xilinx/14.7/ISE_DS/ISE/bin/lin64/unwrapped/fuse -intstyle ise -incremental -lib secureip -o /root/FFT/testbench_isim_beh.exe -prj /root/FFT/testbench_beh.prj work.testbench && "/root/FFT/testbench_isim_beh.exe" -intstyle ise  -wdb "/root/FFT/testbench_isim_beh.wdb" -tclbatch test.cmd  | grep -v truncated; cp /tmp/dds.out /tmp/frames2/dds.$i.out; cp /tmp/xilinx.out /tmp/frames2/xilinx.$i.out; done
